<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--medium-->
    <font
        android:font="@font/pretendardmedium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/pretendardmedium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:targetApi="o" />

    <!--semiBold-->
    <font
        android:font="@font/pretendardsemibold"
        android:fontStyle="normal"
        android:fontWeight="750"
        app:font="@font/pretendardsemibold"
        app:fontStyle="normal"
        app:fontWeight="750"
        tools:targetApi="o" />

</font-family>